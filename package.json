{
  "name": "template_backend",
  "version": "0.0.1",
  "main": "build/src",
  "scripts": {
    "build": "babel src -s -D -d build/src && babel clients -s -D -d build/clients",
    "postinstall": "npm run build",
    "start": "node build/src",
    "prestart": "./node_modules/sequelize-cli/bin/sequelize db:migrate",
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "dependencies": {
    "acl": "^0.4.10",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "mysql": "^2.12.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react-s3-uploader": "^4.1.1",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "3.30.4",
    "winston": "^2.3.1",
    "yup": "^0.21.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "nodemon": "^1.9.2",
    "sequelize-cli": "^2.4.0"
  }
}
